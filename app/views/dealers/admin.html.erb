 <% provide(:title, 'Preferences') %>
 <h1 class='hidden-xs'>Preferences</h1>
 <center><h4 class='visible-xs'>Preferences</h4></center>

<div class='table-width-desktop'>
  <center>
    <button type="button" class="btn btn-default pref-btn-fix" data-toggle="collapse"
            data-target="#billing">
      Billing
    </button>
    <button type="button" class="btn btn-default pref-btn-fix" data-toggle="collapse"
            data-target="#dealership_info">
      Dealership
    </button>
    <button type="button" class="btn btn-default pref-btn-fix" data-toggle="collapse"
            data-target="#export">
      Export
    </button>

    <div id="billing" class="collapse">
      <%= render 'billing' %>
    </div>
    <div id="dealership_info" class="collapse">
     <%= render 'dealership_info' %>
    </div>
    <div id="export" class="collapse">
    <%= render 'export' %>
    </div>
  </center>
</div>
